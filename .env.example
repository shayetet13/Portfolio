# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Site Configuration
VITE_SITE_URL=https://your-domain.com
VITE_SITE_NAME=Your Portfolio Name
VITE_ENVIRONMENT=production

# Telegram Bot Configuration - Contact Form Bot
VITE_TELEGRAM_CONTACT_BOT_TOKEN=your_contact_bot_token_here
VITE_TELEGRAM_CONTACT_CHAT_ID=your_contact_chat_id_here

# Telegram Bot Configuration - Chat Button Bot
VITE_TELEGRAM_CHAT_BOT_TOKEN=your_chat_bot_token_here
VITE_TELEGRAM_CHAT_CHAT_ID=your_chat_chat_id_here

# Legacy Telegram Bot Configuration (Backward compatibility)
VITE_TELEGRAM_BOT_TOKEN=your_legacy_bot_token_here
VITE_TELEGRAM_CHAT_ID=your_legacy_chat_id_here

# Social Media Links
VITE_LINE_ID=your_line_id
VITE_FACEBOOK_URL=https://facebook.com/your_page
VITE_TWITTER_URL=https://twitter.com/your_account
VITE_TIKTOK_URL=https://tiktok.com/@your_account
VITE_TELEGRAM_URL=https://t.me/your_username
VITE_EMAIL=your_email@domain.com

# Analytics (Optional)
VITE_ANALYTICS_ENABLED=true
VITE_TRACKING_ID=your_tracking_id

# Performance & CDN (Optional)
VITE_CDN_URL=https://cdn.netlify.com
VITE_ENABLE_PWA=true
